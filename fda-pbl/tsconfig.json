{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "strict": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./out-tsc"
  },
  "angularCompilerOptions": {
    "enableIvy": true
  }
}